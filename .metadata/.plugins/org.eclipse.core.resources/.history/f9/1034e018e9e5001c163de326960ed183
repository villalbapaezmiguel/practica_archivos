#include <stdio.h>
#include <stdlib.h>
#include <string.h>
#include "UTN.h"
#include "Alumno.h"

int main(void) {

	FILE* pArchivo ;
	char buffer [128];

	pArchivo = fopen("NuevoArchivo.txt", "rb");
	if(pArchivo != NULL)
	{

		fgets(buffer, sizeof(buffer), pArchivo );
		printf("\n %s", buffer);
		fclose(pArchivo);

	}




	return EXIT_SUCCESS;
}
